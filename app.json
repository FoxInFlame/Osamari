{
  "expo": {
    "name": "Osamari",
    "description": "This project is really great.",
    "slug": "Osamari",
    "privacy": "unlisted",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "tk.zucchinasu.osamari",
      "supportsTablet": true
    },
    "android": {
      "package": "tk.zucchinasu.osamari"
    }
  }
}
